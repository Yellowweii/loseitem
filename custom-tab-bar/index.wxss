.tabbar{
	width: 100%;
	display: flex;
	background-color: #fff;
	z-index: 999;
	position: fixed;
	bottom: 0;
}
.tabbar .tabbar-item{
	flex: 1;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.tabbar .img-one{
	width: 50rpx;
	height: 50rpx;
}
.tabbar-item .txt{
	font-size: 26rpx;
}
  .publish .img-two{
		width: 100rpx;
		height: 100rpx;
}
.publish{
		margin-bottom: auto;
}
.tabbar-select{
	color: cornflowerblue;
}